<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
                               "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.sds.emp.services.user.vo.UserVO" alias="userVO"/>
		<typeAlias type="com.sds.emp.services.board.vo.BoardVO" alias="boardVO"/>
		<typeAlias type="com.sds.emp.services.board.vo.SearchVO" alias="searchVO"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="sql/emp-user-userservice-mapping.xml" />
		<mapper resource="sql/emp-board-boardservice-mapping.xml" />
	</mappers>
</configuration>
